{
    "name": "kiwilan/php-tmdb",
    "description": "PHP wrapper package to interact with the The Movie Database (TMDB) API.",
    "version": "0.1.0",
    "keywords": [
        "php",
        "tmdb",
        "movie",
        "api",
        "tv",
        "series"
    ],
    "homepage": "https://github.com/kiwilan/php-tmdb",
    "license": "MIT",
    "authors": [
        {
            "name": "Ewilan Rivi√®re",
            "email": "ewilan.riviere@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "php": "^8.1",
        "laravel/pint": "^1.0",
        "pestphp/pest": "^2.20",
        "phpstan/phpstan": "^1.12",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Kiwilan\\Tmdb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiwilan\\Tmdb\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "test-filter": "vendor/bin/pest --filter",
        "format": "vendor/bin/pint",
        "analyse": "phpstan analyse --ansi --memory-limit=4G"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
